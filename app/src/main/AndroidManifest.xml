<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.im.music">
<uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.VIBRATE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/musicicon"
        android:label="@string/app_name"
        android:name=".database.DbflowApplication"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.RegistrationPage">

        </activity>
        <service android:name=".services.MyService" />
        <receiver android:name=".notification.HandleNotificationIntent">
        <intent-filter>
            <action android:name="com.example.im.music.previous" />
            <action android:name="com.example.im.music.delete" />
            <action android:name="com.example.im.music.pause" />
            <action android:name="com.example.im.music.next" />
            <action android:name="com.example.im.music.play" />
        </intent-filter>
    </receiver>
</application>

</manifest>